#!/usr/bin/tclsh

# associative arrays in tcl act more or less like key value pairs
# in a python dictionary.


# Creating associative arrays
set fruits(red) "strawberry"
# printing the value within an associateive arrays
puts $fruits(red)

set fruits(blue) "blueberry"

puts $fruits(blue)

set fruits(yellow) "banana"

puts $fruits(yellow)

set fruits(orange) "orange"

puts $fruits(orange)














